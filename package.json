{
  "name": "projectname",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack",
    "start": "node dist/index.js",
    "cli": "node dist/cli.js",
    "cli:dev": "npm run build && node dist/cli.js",
    "start:dev": "npm run build && node dist/index.js --inspect=0.0.0.0:9229",
    "start:watch": "concurrently --kill-others \"npm run start:watch:compile\" \"npm run start:watch:run\"",
    "start:watch:compile": "webpack --watch",
    "start:watch:run": "nodemon",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json)\"",
    "formatTest": "prettier --write \"test/**/*.+(js|jsx|ts|tsx|json)\"",
    "test": "jest --watchAll",
    "test:coverage": "jest --watchAll --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:ci": "jest --collect-coverage",
    "test:commit": "jest --collect-coverage --passWithNoTests",
    "test:rpi": "jest --runInBand --watch",
    "publish:test:coverage": "codecov",
    "validate": "npm run format && npm run formatTest && npm run lint && npm run test:ci && npm run build",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "author": "juanjofp",
  "repository": "",
  "license": "MIT",
  "devDependencies": {
    "@types/convict": "^6.1.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "codecov": "^3.8.3",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-filenames": "^1.3.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-shebang-plugin": "^1.1.8"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint ./src --fix",
      "prettier --write ./src",
      "npm run test:commit"
    ]
  },
  "dependencies": {
    "@actions/github": "^5.1.1"
  }
}
